{
  "name": "spotify-library",
  "version": "1.0.0",
  "private": false,
  "description": "Get your Spotify library via CLI",
  "keywords": [
    "spotify",
    "library",
    "music",
    "json",
    "cli",
    "typescript"
  ],
  "homepage": "https://github.com/eegli/spotify-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/eegli/spotify-library.git"
  },
  "license": "MIT",
  "author": {
    "name": "Eric Egli",
    "email": "lx180777+npm@gmail.com"
  },
  "main": "dist/index.js",
  "module": "commonjs",
  "types": "dist/index.d.ts",
  "bin": "cli.js",
  "files": [
    "dist",
    "cli.js",
    "readme.md"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc --build",
    "dev": "tsc --watch",
    "format": "prettier --write src",
    "install:ci": "yarn install --ignore-optional --frozen-lockfile",
    "lint": "eslint src test --ext .ts",
    "prepare": "husky install",
    "prepublish": "yarn build",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.ts": [
      "eslint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cli-progress": "^3.9.1"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.9.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.1",
    "@types/spotify-api": "^0.0.12",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.3",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=12"
  }
}
